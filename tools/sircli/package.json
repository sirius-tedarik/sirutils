{
  "name": "@sirutils/sircli",
  "description": "All in one cli for sirutils",
  "version": "1.0.0",

  "bin": {
    "sircli": "./command"
  },

  "type": "module",
  "files": ["dist", "./command"],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./main": {
      "source": "./src/main.ts",
      "default": "./dist/main.js",
      "types": "./dist/main.d.ts"
    }
  },

  "author": "giveerr (https://github.com/giveerr)",
  "homepage": "https://sirutils.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/sirius-tedarik/sirutils.git"
  },

  "dependencies": {
    "clerc": "^0.44.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "typescript": ">= 5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
